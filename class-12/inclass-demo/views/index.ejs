<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('./layout/head') %>
</head>
<body>
  <header>
    <%- include('./layout/header') %>
  </header>
  <h1>Notes go here:</h1>
  <section>
    <ul>
      <% tasks.forEach(task => { %>
        <li>
          <h4>Task:  <%= task.title %></h4>
          <p>Completion status: <%= task.status %></p>
          <a href="/tasks/<%= task.id %>">View details</a>
        </li>
      <% }) %>
    </ul>
  </section>
</body>
</html>